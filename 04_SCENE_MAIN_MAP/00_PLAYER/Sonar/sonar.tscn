script/source = "extends Node


@onready var _1 = $\"../01\"
@onready var _2 = $\"../02\"
@onready var sonar = $\"..\"

func _ready():
	
	sonar.scale = Vector3(0.1,0.1,0.1)
	_1.visible = false
	_2.visible = false
	
	active_sonar()
	
func active_sonar():


	_1.visible = true
	_2.visible = true
	
	var t 
	t = create_tween()
	
	t .tween_method(_change_scale,0.0,7.0,1.5).set_ease(Tween.EASE_OUT)
	t.connect(\"finished\",_stop)


func _change_scale(value : float):
	sonar.scale = Vector3(value,value,value)
	
func _stop():
	
	sonar.scale = Vector3(0.1,0.1,0.1)
	_1.visible = false
	_2.visible = false
	
	sonar.queue_free()


"

